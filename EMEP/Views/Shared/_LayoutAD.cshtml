<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - EMEP</title>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
</head>
<body>

    @*MENU*@
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary">

        @Html.ActionLink("EMEP", "IndexAd", "Home", new { area = "" }, new { @class = "navbar-brand" })
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="# navbarColor01" aria-controls="navbarColor01" aria-expand="false" aria-label="Navegación de palanca ">
            <span class="navbar-toggler-icon"> </span>
        </button>

        <div class="collapse navbar-collapse" id="navbarColor01">
            <ul class="navbar-nav mr-auto">

                <li class="nav-item">

                    @Html.ActionLink("Inicio", "IndexAd", "Home", new { area = "" }, new { @class = "nav-link" })
                    @*@Html.ActionLink("Inicio", "IndexAd", "Home", new { area = "" }, new { @class = "nav-link" })*@
                </li>
                <li class="nav-item">

                    @Html.ActionLink("Administradores", "Index", "Administrador", new { area = "" }, new { @class = "nav-link" })
                    @*<a class="nav-link" href="#"> <font style="vertical-align: inherit;"> <font style="vertical-align: inherit;"> Tour </font> </font> </a>*@
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Medicos", "Index", "Medico", new { area = "" }, new { @class = "nav-link" })
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Consulta", "Index", "Consulta", new { area = "" }, new { @class = "nav-link" })
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Consultorio", "Index", "Consultorio", new { area = "" }, new { @class = "nav-link" })
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Especialidad", "Index", "Especialidad", new { area = "" }, new { @class = "nav-link" })
                </li>
            </ul>
            @Html.Partial("_PartialAdmin")
          
        </div>
    </nav>
    @*MENU*@


    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Derechos Reservados - EMEP</p>
        </footer>
    </div>


    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>